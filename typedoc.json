{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["./src"],
    "entryPointStrategy": "expand",
    "out": "docs",
    "readme": "README.md",
    "name": "InterMind Documentation",
    "includeVersion": true,
    "excludePrivate": true,
    "excludeProtected": false,
    "excludeExternals": true,
    "excludeInternal": true,
    "disableSources": false,
    "searchInComments": true,
    "hideGenerator": true,
    "skipErrorChecking": true,
    
    "navigationLinks": {
        "GitHub": "https://github.com/lbatschelet/InterMind"
    },
    
    "sort": ["static-first", "alphabetical"],
    
    "validation": {
        "invalidLink": true,
        "notExported": true
    },
    
    "blockTags": [
        "@example", 
        "@remarks", 
        "@summary", 
        "@module",
        "@returns",
        "@param",
        "@throws",
        "@template",
        "@category",
        "@constant",
        "@description",
        "@packageDocumentation"
    ],
    
    "categorizeByGroup": false,
    "defaultCategory": "Other",
    
    "cleanOutputDir": true
} 