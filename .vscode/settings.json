{
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": [
    "bare-node-builtins",
    "byonm",
    "sloppy-imports",
    "unsafe-proto",
    "webgpu",
    "broadcast-channel",
    "worker-options",
    "cron",
    "kv",
    "ffi",
    "fs",
    "http",
    "net"
  ],
  "treeMaker.files.outputFileName": "structure",
  "treeMaker.files.outputFilePrefix": "",
  "treeMaker.files.outputFileSeparator": "_",
  "treeMaker.files.outputFileSuffix": "",
  "treeMaker.files.outputFolder": "tree-maker",
  "treeMaker.files.selectedOutputFormat": "markdown",
  "treeMaker.search.ignoreFilePathPatternOnExport": [
    "**/dist/**",
    "**/tree-maker/**"
  ],
  "treeMaker.search.keepGitignorePreferences": false,
  "treeMaker.search.maxSearchRecursionDepth": 5,
  "treeMaker.search.shouldListOnlyFiles": false,
  "treeMaker.search.shouldListOnlyVisibleFiles": true
}
