{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["../src"],
  "entryPointStrategy": "expand",
  "out": "../docs",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "markdown",
  "readme": "../README.md",
  "name": "SerenCity API Documentation",
  "includeVersion": true,
  "categorizeByGroup": true,
  "searchInComments": true,
  "hideGenerator": true,
  "visibilityFilters": {
    "protected": true,
    "private": false,
    "inherited": true,
    "external": false
  },
  "navigationLinks": {
    "GitHub": "https://github.com/yourusername/SerenCity"
  },
  "sort": ["source-order"],
  "validation": {
    "invalidLink": true,
    "notExported": true
  }
} 